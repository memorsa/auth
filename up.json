{
  "name": "memorsa-auth",
  "profile": "lambda",
  "regions": [
    "us-east-1"
  ],
  "proxy": {
    "command": "./auth-server"
  },
  "hooks": {
    "build": [
      "cargo build --release --target x86_64-unknown-linux-musl",
      "cp target/x86_64-unknown-linux-musl/release/server ./auth-server"
    ]
  },
  "clean": "rm auth-server"
}
